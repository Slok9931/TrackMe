    "express-session": "^1.18.2",
    "mongoose": "^7.6.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.8.0",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
