{
  "name": "express-server",
  "version": "1.0.0",
  "description": "An Express server with MongoDB connection and Google OAuth authentication",
  "main": "src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run clean && tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "clean": "rm -rf dist",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
